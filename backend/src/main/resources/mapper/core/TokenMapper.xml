<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nexus.backend.domain.token.mapper.TokenMapper">
    <select id="getDesignToken" resultType="com.nexus.backend.domain.token.NxDesignToken">
        SELECT TOKEN_ID, TOKEN_NAME, TOKEN_VALUE, USE_YN
        FROM NX_DESIGN_TOKEN
        WHERE TOKEN_ID = #{tokenId} AND USE_YN = 'Y'
    </select>
</mapper>